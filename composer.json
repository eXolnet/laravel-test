{
	"name": "exolnet/laravel-test",
	"description": "The eXolnet Laravel Test package.",
	"license": "MIT",
	"keywords": ["exolnet", "laravel", "test"],
	"authors": [
		{
			"name": "Tom Rochette",
			"email": "trochette@exolnet.com"
		}
	],
	"require": {
		"php": ">=7.0.0",
		"fzaninotto/faker": "^1.6",
		"laravel/browser-kit-testing": "^3.0|^4.0|^5.0",
		"laravel/framework": "5.5.*|5.6.*|5.7.*|5.8.*|^6.0",
		"mockery/mockery": "^1.0",
		"phpunit/phpunit": "^6.0|^7.0|^8.0"
	},
	"autoload": {
		"psr-4": {
			"Exolnet\\Test\\": ""
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "2.1-dev"
		}
	},
	"minimum-stability": "stable",
	"require-dev": {
	},
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
