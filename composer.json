{
    "name": "exolnet/laravel-test",
    "description": "The eXolnet Laravel Test package.",
    "license": "MIT",
    "keywords": [
        "exolnet",
        "laravel",
        "test"
    ],
    "authors": [
        {
            "name": "Tom Rochette",
            "email": "trochette@exolnet.com"
        },
        {
            "name": "Simon Gaudreau",
            "email": "sgaudreau@exolnet.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "fzaninotto/faker": "^1.6",
        "laravel/browser-kit-testing": "^5.0",
        "laravel/framework": "^6.0",
        "laravel/helpers": "^1.1",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "Exolnet\\Test\\": ""
        }
    },
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
